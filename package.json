{
  "name": "amqpuri",
  "version": "1.1.0",
  "codename": "Relocated Rabbit",
  "description": "Create AMQP URIs the node way",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "engines": {
    "node": "> 6.0.0"
  },
  "scripts": {
    "coverage": "nyc npm run test",
    "test": "NODE_ENV=test mocha --reporter spec --bail test/**/*.test.js",
    "lint": "eslint index.js test/",
    "validate": "npm-run-all lint coverage",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "format": "prettier --write index.js test/**/*.js",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfantone/amqp-uri.git"
  },
  "keywords": [
    "amqp",
    "uri",
    "url",
    "rabbitmq",
    "activemq",
    "zmq"
  ],
  "author": "Nicol√°s Fantone <nfantone@gmail.com> (https://github.com/nfantone)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nfantone/amqp-uri/issues"
  },
  "homepage": "https://github.com/nfantone/amqp-uri#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-semistandard": "^12.0.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "husk": "^0.5.3",
    "lint-staged": "^6.0.1",
    "mocha": "^5.0.0",
    "mocha-junit-reporter": "^1.16.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.1",
    "prettier": "^1.10.2"
  },
  "dependencies": {
    "lodash.curry": "^4.1.1",
    "lodash.flowright": "^3.5.0",
    "lodash.pick": "^4.4.0"
  }
}
