{
  "name": "amqpuri",
  "version": "2.0.1",
  "description": "Create AMQP URIs the node way",
  "main": "index.js",
  "files": [
    "index.js",
    "lib/"
  ],
  "engines": {
    "node": "> 6.0.0"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "report-coverage": "codecov",
    "lint": "eslint index.js test/ lib/",
    "validate": "npm-run-all lint coverage",
    "format": "prettier --write index.js lib/**/*.js test/**/*.js",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfantone/amqp-uri.git"
  },
  "keywords": [
    "amqp",
    "uri",
    "url",
    "rabbitmq",
    "activemq",
    "zmq"
  ],
  "author": "Nicol√°s Fantone <nfantone@gmail.com> (https://github.com/nfantone)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nfantone/amqp-uri/issues"
  },
  "homepage": "https://github.com/nfantone/amqp-uri#readme",
  "devDependencies": {
    "@types/jest": "^23.0.2",
    "ajv": "^6.5.1",
    "codecov": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^3.0.0",
    "eslint-config-semistandard": "^12.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^2.0.0",
    "jest": "^23.1.0",
    "lint-staged": "^9.4.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.5"
  },
  "dependencies": {
    "lodash.curry": "^4.1.1",
    "lodash.flowright": "^3.5.0",
    "lodash.pick": "^4.4.0"
  }
}
